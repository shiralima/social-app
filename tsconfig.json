{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@stores/*": [
        "src/stores/*"
      ]
    }
  },
  "include": [
    "src",
    "__tests__",
    "__tests__/.test.tsx"
, "App.tsx"  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
